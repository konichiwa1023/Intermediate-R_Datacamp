# The linkedin vector has already been defined for you
linkedin <- c(16, 9, 13, 5, 2, 17, 14)

# Code the for loop with conditionals
for (li in linkedin) {
  if (li > 10) {
    print("You're popular!")  # Add this print statement
  } else {
    print("Be more visible!")  # Add this print statement
  }
  print(li)
}
